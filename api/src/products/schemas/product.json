{
  "$id": "/schemas/product",
  "$async": true,
  "title": "Product",
  "description": "Product schema",
  "type": "object",
  "properties": {
    "id": {
      "readOnly": true,
      "type": "string",
      "minLength": 30,
      "maxLength": 30
    },
    "title": {
      "type": "string",
      "minLength": 1,
      "maxLength": 255
    },
    "description": {
      "type": "string",
      "minLength": 1,
      "maxLength": 255
    },
    "qr": {
      "type": "string",
      "minLength": 1,
      "maxLength": 255
    },
    "sku": {
      "type": "string",
      "minLength": 1,
      "maxLength": 100
    },
    "created_at": {
      "readOnly": true,
      "type": "string"
    },
    "updated_at": {
      "readOnly": true,
      "type": "string"
    },
    "variant_id": {
      "type": "string",
      "minLength": 30,
      "maxLength": 30
    },
    "category_id": {
      "type": "string",
      "minLength": 30,
      "maxLength": 30
    },
    "shop_id": {
      "type": "string",
      "minLength": 30,
      "maxLength": 30
    },
    "quantity": {
      "type": "integer",
      "exclusiveMinimum": 0,
      "default": 1
    }
  },
  "required": ["title", "sku"],
  "additionalProperties": false
}
